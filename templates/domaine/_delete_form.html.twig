<form method="post" action="{{ path('app_domaine_delete', {'id': domaine.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer le domaine {{ domaine.nom }} ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ domaine.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
