<form method="post" action="{{ path('app_protocole_delete', {'id': protocole.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer le protocole {{ protocole.nom }} ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ protocole.id) }}">
    <button class="btn btn-outline-danger">Supprimer</button>
</form>
