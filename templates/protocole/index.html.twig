{% extends 'base.html.twig' %}

{% block title %}Protocole index{% endblock %}

{% block body %}
    <h1>Protocole index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Fichier</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for protocole in protocoles %}
            <tr>
                <td>{{ protocole.id }}</td>
                <td>{{ protocole.nom }}</td>
                <td>{{ protocole.fichier }}</td>
                <td>
                    <a href="{{ path('app_protocole_show', {'id': protocole.id}) }}">show</a>
                    <a href="{{ path('app_protocole_edit', {'id': protocole.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_protocole_new') }}">Create new</a>
{% endblock %}
